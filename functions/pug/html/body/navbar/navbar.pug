nav.navbar.navbar-expand-md.bg-success.pl-4
  a.navbar-brand.text-light(
    title= translation.welcome.title1
    href=`/${ lang }`
  ) PERMADATA
  button.navbar-toggler(
    type='button'
    data-toggle='collapse'
    data-target='#navbar-content'
    aria-controls='navbar-content'
    aria-expanded='false'
    aria-label='toggle navigation')
    span(class='navbar-toggler-icon')
  #navbar-content.collapse.navbar-collapse
    ul.navbar-nav.mr-auto
    ul.navbar-nav.mt-4.mt-sm-0
      li#launch-info.alert.alert-warning.mb-0.py-2.mx-1
        p.mb-0.px-2.text-uppercase= `${ translation.loading.label } : `
          span#launch-info__status= translation.loading.initial
      li#error-info.mx-1.my-2.my-md-0.d-none
        #error-info__buttons.btn-group(
          role='group'
          aria-label='error group'
        )
          button#error-button.btn.btn-danger.text-uppercase.py-2.col-12.px-3= `${ translation.loading['error label'] } : `
            span#error-info__status= translation.noError
      li#login-button-container.nav-item.mx-1.my-2.my-md-0.d-none
        button#login-button.btn.btn-warning.text-uppercase.py-2.px-4.col-12= `${ translation.authenticate } 👩🏾‍🌾 🥝👨🏻‍🌾`
      li#anchor-user-panel.nav-item.mx-1.alert.alert-success.border.border-success.mb-0.py-2.pt-2.my-2.my-md-0.d-none
      li#search-plant-button-container.bundle-true.nav-item.mx-1.my-2.my-md-0.d-none
        button#search-plant-button.btn.btn-light.text-dark.text-uppercase.py-2.px-4.col-12= `${ translation.searchPlant }  🌱 📚`
      li.nav-item.mx-1.my-2.my-md-0
        a.btn.btn-outline-light.nav-link.text-uppercase.px-3(
          href=`/${ lang === 'en' ? 'fr' : 'en' }${ url }`
        )= `${ lang === 'en' ? 'fr' : 'en' }`