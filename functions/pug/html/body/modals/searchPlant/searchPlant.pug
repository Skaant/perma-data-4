#search-plant-modal.modal(role='document')
  .modal-dialog(role='document')
    .modal-content
      .modal-header.bg-warning
        h5.modal-title.text-dark.text-uppercase= translation.plantSearchModal.title
        button.close(type='button' data-dismiss='modal' aria-label='Close')
          span(aria-hidden='true') &times;
      .modal-body.container
        .anchor-plant-search.mt-4.col-12.col-md-10.offset-md-1